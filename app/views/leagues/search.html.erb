<%= render "common/menu" %>
<div class="container">
  <p class="mb-5">
    Found <%= pluralize(@leagues.count, 'league') %>  found when searching for: <%= params[:keywords] %>
  </p>
  <ul>
    <% @leagues.each do |league| %>
      <li><%= link_to league.name, league %></li>
    <% end %>
  </ul>
</div>
